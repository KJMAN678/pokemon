{
    "name": "pokemon",
    "dockerComposeFile": "../compose.yml",
    "service": "pokemon-showdown",
    "workspaceFolder": "/app/",
    "forwardPorts": [8000],
    "mounts": [
      "source=${localEnv:LOCAL_APP_PATH},target=/app,type=bind,consistency=cached"
    ],
    "customizations": {
      "vscode": {
        "extensions": [
            "ms-toolsai.jupyter",
            "ms-python.python"
        ]
      }
    }
  }
